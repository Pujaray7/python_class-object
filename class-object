class myClass:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def myFunc(self):
        print(self.name, self.age )
p1=myClass("Puja",17)
p1.myFunc()
class newClass(myClass):
    def __init__(self,name,age,year):
        super().__init__(name,age)
        self.dob=year
    def newFunc(self):
        print(self.name,self.age,self.dob)
        
s1=newClass("Nisha",18,2004)
s1.newFunc()

# print(s1.dob)
